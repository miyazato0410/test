		Person taro=new Person();
			taro.name="山田太郎";
			taro.age=20;

		Person kimura=new Person();
			kimura.name="木村次郎";
			kimura.age=18;

		Person suzuki=new Person();
			suzuki.name="鈴木花子";
			suzuki.age=16;


		System.out.println(taro.name);
		System.out.println(taro.age);

		System.out.println(kimura.name);
		System.out.println(kimura.age);

	}
